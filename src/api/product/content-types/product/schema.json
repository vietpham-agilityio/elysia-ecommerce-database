{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text",
      "required": true
    },
    "imageSrc": {
      "type": "text",
      "required": true
    },
    "color": {
      "type": "string",
      "default": "Red"
    },
    "price": {
      "type": "integer",
      "default": 1782,
      "required": true
    },
    "capacity": {
      "type": "integer"
    },
    "stock": {
      "type": "biginteger",
      "required": true,
      "default": "100"
    },
    "warranty": {
      "type": "biginteger",
      "required": true,
      "default": "2"
    },
    "material": {
      "type": "string",
      "default": "wood"
    },
    "type": {
      "type": "string"
    },
    "shippingInfo": {
      "type": "string",
      "default": "VN Express"
    }
  }
}
